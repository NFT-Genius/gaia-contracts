    "csv-parser": "^3.0.0",
    "csv-writer": "^1.6.0",
    "dotenv": "^10.0.0",
    "elliptic": "^6.5.4",
    "esm": "^3.2.25",
    "sha3": "^2.1.4",
    "yargs": "^17.0.1"
  },
  "devDependencies": {
    "@onflow/dev-wallet": "^0.0.10",
    "eslint": "^7.23.0",
    "eslint-config-prettier": "^8.1.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-prettier": "^3.3.1",
    "esm": "^3.2.25",
    "husky": "4.3.8",
    "lint-staged": "^11.0.0",
